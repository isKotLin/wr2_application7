        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">@dimen/my100dp</item>
        <item name="android:textSize">@dimen/my110dp</item>
        <!--item name="android:textStyle">bold</item-->
    </style>

    <style name="setting_userinfo">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">#180303</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:padding">@dimen/my10dp</item>
        <item name="android:textSize">@dimen/my30dp</item>
    </style>
</resources>