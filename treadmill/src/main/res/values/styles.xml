<resources>

    <!-- Base application theme. -->
    <style name="fade_animation">
        <item name="@android:windowEnterAnimation">@anim/popup_show</item>
        <item name="@android:windowExitAnimation">@anim/popup_hide</item>
    </style>
    <style name="AppTheme" parent="Theme.AppCompat">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="defaultDialogStyle" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.5</item>
    </style>
    <style name="Transparent">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@color/transparentss</item>
    </style>

    <style name="Transp">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@color/transparentss</item>
    </style>

    <style name="Custom_Dialog_Theme" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowNoTitle">true</item>
        <!-- 隐藏标题栏 -->
    </style>

    <style name="user_name_ll">
        <item name="android:layout_width">@dimen/my200dp</item>
        <item name="android:layout_height">@dimen/my310dp</item>
        <item name="android:clickable">true</item>
        <item name="android:orientation">vertical</item>
        <item name="android:soundEffectsEnabled">true</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginRight">@dimen/my10dp</item>
    </style>

    <style name="user_icon">
        <item name="android:layout_width">@dimen/my200dp</item>
        <item name="android:layout_height">@dimen/my250dp</item>
    </style>

    <style name="user_name_tv_show">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/my34dp</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="custom">
        <item name="android:layout_height">@dimen/my220dp</item>
        <item name="android:gravity">center</item>
        <item name="android:clickable">true</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/my40dp</item>
    </style>

    <style name="custom1">
        <item name="android:layout_height">@dimen/my110dp</item>
        <item name="android:layout_width">@dimen/my220dp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginLeft">@dimen/my75dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/my40dp</item>
    </style>

    <style name="custom1_wv">
        <item name="android:layout_width">@dimen/my200dp</item>
        <item name="android:layout_height">@dimen/my400dp</item>
        <item name="android:layout_marginLeft">@dimen/my100dp</item>
        <item name="android:layout_marginTop">@dimen/my24dp</item>
    </style>

    <style name="strength">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/my80dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/my30dp</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="rb_per">
        <item name="android:layout_width">@dimen/my150dp</item>
        <item name="android:layout_height">@dimen/my90dp</item>
        <item name="android:layout_marginLeft">@dimen/my16dp</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/my30dp</item>
    </style>

    <style name="number">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">@dimen/my10dp</item>
        <item name="android:textSize">@dimen/my30dp</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="customs">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/my24dp</item>
    </style>

    <style name="error_codes">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:layout_margin">@dimen/my5dp</item>
        <item name="android:layout_marginBottom">@dimen/my5dp</item>
        <item name="android:layout_marginRight">@dimen/my5dp</item>
        <item name="android:layout_marginLeft">@dimen/my5dp</item>
        <item name="android:layout_marginTop">@dimen/my15dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/my30dp</item>
    </style>

    <style name="error_type">
        <item name="android:layout_width">@dimen/my450dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:layout_marginBottom">@dimen/my5dp</item>
        <item name="android:layout_marginRight">@dimen/my5dp</item>
        <item name="android:layout_marginLeft">@dimen/my5dp</item>
        <item name="android:layout_marginTop">@dimen/my15dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/my30dp</item>
    </style>

    <style name="error_opear">
        <item name="android:layout_width">@dimen/my660dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:layout_marginBottom">@dimen/my5dp</item>
        <item name="android:layout_marginRight">@dimen/my5dp</item>
        <item name="android:layout_marginLeft">@dimen/my5dp</item>
        <item name="android:layout_marginTop">@dimen/my15dp</item>
        <item name="android:textSize">@dimen/my30dp</item>
    </style>

    <style name="user">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">@dimen/my4dp</item>
        <item name="android:background">@drawable/selector_user_radio</item>
        <item name="android:button">@null</item>
        <item name="android:paddingBottom">@dimen/my10dp</item>
        <item name="android:paddingLeft">@dimen/my20dp</item>
        <item name="android:paddingRight">@dimen/my20dp</item>
        <item name="android:paddingTop">@dimen/my10dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/my30dp</item>
    </style>

    <style name="lastll">
        <item name="android:layout_width">@dimen/my500dp</item>
        <item name="android:layout_height">@dimen/my150dp</item>
        <item name="android:layout_marginLeft">@dimen/my15dp</item>
        <item name="android:layout_marginTop">@dimen/my10dp</item>
        <item name="android:background">@drawable/share_frame</item>
    </style>

    <style name="lastiv">
        <item name="android:layout_width">@dimen/my40dp</item>
        <item name="android:layout_height">@dimen/my40dp</item>
    </style>

    <style name="lasttv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/my50dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/my24dp</item>
    </style>

    <style name="lasttv1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/share_dis</item>
        <item name="android:textSize">@dimen/my20dp</item>
        <item name="android:layout_alignParentRight">true</item>
