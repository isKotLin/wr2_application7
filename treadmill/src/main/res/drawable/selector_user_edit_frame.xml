<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true">
        <shape>
            <solid android:color="@android:color/white"/>
            <size android:width="@dimen/my170dp" android:height="@dimen/my50dp"/>
            <stroke android:width="@dimen/my5dp" android:color="#F3992d"/>
        </shape>
    </item>
    <item android:state_focused="false">
        <shape>
            <solid android:color="@android:color/white"/>
            <size android:width="@dimen/my170dp" android:height="@dimen/my50dp"/>
            <stroke android:width="@dimen/my5dp" android:color="#878181"/>
        </shape>
    </item>
</selector>